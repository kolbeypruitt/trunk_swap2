extends ../layout

block content
  .container
    .row
      .col-md-3
      .col-md-6
        h1 Search Results are...
        table
          thead
            tr
              th ID
              th Year
              th Model
              th Style
          tbody
            for trunk in allTrunks
              tr
                td
                  a(href='/trunks/' + trunk._id)= trunk._id
                td= trunk.current_year
                td= trunk.current_model
                td= trunk.current_style
      .col-md-3